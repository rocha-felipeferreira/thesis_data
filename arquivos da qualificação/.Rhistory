enfasesPresidentes_continente <- gather(enfasesPresidentes_continente, key = "categorias",
value = "valor", -document)
enfasesPresidentes_continente$categorias <- as.factor(enfasesPresidentes_continente$categorias)
ggplot(enfasesPresidentes_continente, aes(x=categorias, y=valor, fill = document)) +
geom_col() + coord_flip()
